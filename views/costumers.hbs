<h1 class="h1">Kunder</h1>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Unikt ID</th>
            <th>KID</th>
            <th>Password</th>
            <th>CPR-nummer</th>
            <th>Navn</th>
            <th>Adresse</th>
            <th>JSON</th>
            <th>Fjern</th>
        </tr>
    </thead>
    {{#each costumers}}
    <tr>
        <td>{{_id}}</td>
        <td>{{cid}}</td>
        <td>{{password}}</td>
        <td>{{security_number}}</td>
        <td>{{name}}</td>
        <td>{{address}}</td>
        <td>
            <form action="/costumers/json/{{cid}}" method="GET">
                <button id="jsonBtn">GET</button>
            </form>
        </td>
        <td>
            <form action="/costumers/delete/{{_id}}" method="POST">
                <button id="deleteBtn">DELETE</button>
            </form>
        </td>

    </tr>
    {{/each}}

</table>